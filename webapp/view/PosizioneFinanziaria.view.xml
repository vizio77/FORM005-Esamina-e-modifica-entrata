							<HBox justifyContent="End">
								<Text
									text="{ path: 'ZSS4_COBI_PREN_ESAMOD_SRV>Competenza25', type:'sap.ui.model.type.Float' ,formatOptions: {style: 'standard', decimals: 2, shortDecimals: 2}}"
									wrapping="true"/>
							</HBox>
						</t:template>
					</t:Column>
					<t:Column width="11rem">
						<Label wrapping="true" text="{i18n>stanzCassaAnno1}&#xA; {i18n>2023}" class="boldCss"/>
						<t:template>
							<HBox justifyContent="End">
								<Text
									text="{ path: 'ZSS4_COBI_PREN_ESAMOD_SRV>Cassa23', type:'sap.ui.model.type.Float' ,formatOptions: {style: 'standard', decimals: 2, shortDecimals: 2}}"
									wrapping="true"/>
							</HBox>
						</t:template>
					</t:Column>
					<t:Column width="11rem">
						<Label wrapping="true" text="{i18n>stanzCassaAnno2} &#xA; {i18n>2024}" class="boldCss"/>
						<t:template>
							<HBox justifyContent="End">
								<Text
									text="{ path: 'ZSS4_COBI_PREN_ESAMOD_SRV>Cassa24', type:'sap.ui.model.type.Float' ,formatOptions: {style: 'standard', decimals: 2, shortDecimals: 2}}"
									wrapping="true"/>
							</HBox>
						</t:template>
					</t:Column>
					<t:Column width="11rem">
						<Label wrapping="true" text="{i18n>stanzCassaAnno3}&#xA; {i18n>2025}" class="boldCss"/>
						<t:template>
							<HBox justifyContent="End">
								<Text
									text="{ path: 'ZSS4_COBI_PREN_ESAMOD_SRV>Cassa25', type:'sap.ui.model.type.Float' ,formatOptions: {style: 'standard', decimals: 2, shortDecimals: 2}}"
									wrapping="true"/>
							</HBox>
						</t:template>
					</t:Column>
				</t:columns>
			</t:TreeTable>
		</f:content>
		<f:footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button type="Emphasized" text="{i18n>NuovaPosFin}" press="onNavNuovaPosFin"/>
				<Button type="Emphasized" text="{i18n>Home}" press="navToAppLaunchpad('Z_S4_ESAMODEPRP')"/>
			</OverflowToolbar>
		</f:footer>
